@model List<NewsManagement.Application.Contrasts.News.NewsViewModel>
<div class="breaking-news-area d-flex align-items-center">
	<div class="news-title">
		<p>تیتر جدیدترین اخبار :</p>
	</div>
	<div id="breakingNewsTicker" class="ticker">
		<ul>
			@foreach (var item in Model)
			{
				<li><a href="/News/@item.Id/@item.Url">@item.Title</a></li>
			}

		</ul>
	</div>
</div>